        "move-cli": "~2.0.0",
        "@types/jest": "~28.1.3",
        "jest": "~28.1.1",
        "ts-jest": "~28.0.5",
        "@types/semver": "~7.3.10"
    },
    "peerDependencies": {
        "discord.js": "~13.8.0"
